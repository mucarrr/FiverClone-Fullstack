{
  "name": "fiverr-clone",
  "version": "1.0.0",
  "description": "Fiverr Clone Full Stack Application",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server": "cd api && npm start",
    "server:dev": "cd api && npm run dev",
    "client": "cd client && npm run dev",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd api && npm install && cd ../client && npm install"
  },
  "keywords": ["fiverr", "clone", "fullstack", "react", "nodejs"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
